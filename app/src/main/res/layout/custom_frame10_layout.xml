<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/_16sdp"
        android:background="@drawable/bg_only_brdr_3_frm10"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"/>

    </LinearLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="@dimen/_8sdp"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginEnd="@dimen/_8sdp"
        android:layout_marginBottom="@dimen/_8sdp"
        android:paddingHorizontal="@dimen/_10sdp">

        <ImageView
            android:id="@+id/iv_logolayout10"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"
            android:scaleType="centerInside" />

        <TextView
            android:id="@+id/tv_f10_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/iv_logolayout10"
            android:layout_marginHorizontal="10dp"
            android:paddingLeft="@dimen/_3sdp"
            android:paddingTop="@dimen/_1sdp"
            android:paddingRight="@dimen/_3sdp"
            android:paddingBottom="@dimen/_1sdp"
            android:text="Name"
            android:textColor="@color/colorBlack"
            android:textStyle="bold"
            android:textSize="@dimen/_11sdp" />

        <LinearLayout
            android:id="@+id/ll_f10_call"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/_20sdp"
            android:background="@drawable/bg_grdiant_cornr"
            android:backgroundTint="@color/frm_10_bg2"
            android:orientation="horizontal"
            android:paddingHorizontal="@dimen/_20sdp"
            android:paddingVertical="@dimen/_5sdp">

            <ImageView
                android:id="@+id/iv_calllayout10"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_marginRight="4dp"
                android:background="@drawable/bg_round"
                android:backgroundTint="@color/frm_10_1"
                android:padding="@dimen/_3sdp"
                android:src="@drawable/mobile"
                android:tint="@color/frm_10_bg2" />

            <TextView
                android:id="@+id/tv_calllayout10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="+91 9876543210"
                android:textColor="@color/colorWhite"
                android:textSize="9dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_20sdp"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/ll_f10_web"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:background="@drawable/bg_grdiant_cornr"
                        android:backgroundTint="@color/frm_10_bg1"
                        android:orientation="horizontal"
                        android:padding="@dimen/_5sdp">

                        <ImageView
                            android:id="@+id/iv_weblayout10"
                            android:layout_width="@dimen/_15sdp"
                            android:layout_height="@dimen/_15sdp"
                            android:layout_marginRight="4dp"
                            android:background="@drawable/bg_round"
                            android:backgroundTint="@color/frm_10_1"
                            android:padding="@dimen/_3sdp"
                            android:src="@drawable/web"
                            android:tint="@color/frm_10_bg2" />

                        <TextView
                            android:id="@+id/tv_websitelayout10"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="www.companywebsite.com"
                            android:textColor="@color/colorWhite"
                            android:textSize="10dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_f10_email"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:background="@drawable/bg_grdiant_cornr"
                        android:backgroundTint="@color/frm_10_bg1"
                        android:orientation="horizontal"
                        android:padding="@dimen/_5sdp">

                        <ImageView
                            android:id="@+id/iv_emaillayout10"
                            android:layout_width="@dimen/_15sdp"
                            android:layout_height="@dimen/_15sdp"
                            android:layout_marginRight="4dp"
                            android:background="@drawable/bg_round"
                            android:backgroundTint="@color/frm_10_1"
                            android:padding="@dimen/_3sdp"
                            android:src="@drawable/mail"
                            android:tint="@color/frm_10_bg2" />

                        <TextView
                            android:id="@+id/tv_emaillayout10"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="companyemail@gmail.com"
                            android:textColor="@color/colorWhite"
                            android:textSize="10dp" />
                    </LinearLayout>


                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/ll_f10_location"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:background="@drawable/bg_grdiant_cornr"
                    android:backgroundTint="@color/frm_10_bg2"
                    android:gravity="center_horizontal"
                    android:orientation="horizontal"
                    android:padding="@dimen/_5sdp">

                    <ImageView
                        android:id="@+id/iv_locationlayout10"
                        android:layout_width="@dimen/_15sdp"
                        android:layout_height="@dimen/_15sdp"
                        android:layout_marginRight="4dp"
                        android:background="@drawable/bg_round"
                        android:backgroundTint="@color/frm_10_1"
                        android:padding="@dimen/_3sdp"
                        android:src="@drawable/location"
                        android:tint="@color/frm_10_bg2" />

                    <TextView
                        android:id="@+id/tv_locationlayout10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="companyemail@gmail.com"
                        android:textColor="@color/colorWhite"
                        android:textSize="10dp" />
                </LinearLayout>


            </LinearLayout>
            <!-- <ImageView
                 android:layout_width="match_parent"
                 android:layout_height="2dp"
                 android:layout_marginLeft="5dp"
                 android:layout_marginRight="5dp"
                 android:layout_marginTop="3dp"
                 android:layout_marginBottom="3dp"
                 android:src="@color/colorBlack">

             </ImageView>-->


            <!--  <LinearLayout
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:orientation="horizontal">
                  <ImageView
                      android:layout_width="20dp"
                      android:layout_height="20dp"
                      android:src="@drawable/f_location"
                      android:layout_marginRight="4dp">
                  </ImageView>
                  <TextView
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:text="Company Address"
                      android:textColor="@color/colorBlack"
                      android:textSize="15dp"></TextView>
              </LinearLayout>-->


        </LinearLayout>
    </RelativeLayout>


</RelativeLayout>